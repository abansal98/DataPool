<div class="default-container">
    <div id="paginatedTable" class="table-responsive" style="max-height: 500px; overflow-y: scroll; overflow-x: auto;">
        <table id="dataTable" class="table table-responsive table-striped table-bordered" style="border: 1px solid black">
            <thead class="thead">
                <tr>
                    <th style="overflow: hidden">Client ID</th>
                    <th style="overflow: hidden">Process Code</th>
                    <th style="overflow: hidden">Data Field 1</th>
                    <th style="overflow: hidden">Data Field 2</th>
                    <th style="overflow: hidden">Data Field 3</th>
                    <th style="overflow: hidden">Data Field 4</th>
                    <th style="overflow: hidden">Data Field 5</th>
                </tr>
            </thead>
            <tbody></tbody>
            <colgroup>
                <col span="9">
                <col>
            </colgroup>
            {{#each data}}
            <tr>
                <td style="overflow: hidden">{{clientId}}</td>
                <td style="overflow: hidden">{{processCode}}</td>
                <td style="overflow: hidden">{{dataField1}}</td>
                <td style="overflow: hidden">{{dataField2}}</td>
                <td style="overflow: hidden">{{dataField3}}</td>
                <td style="overflow: hidden">{{dataField4}}</td>
                <td style="overflow: hidden">{{dataField5}}</td>
            </tr>
            {{/each}}
        </table>
    </div>
</div>